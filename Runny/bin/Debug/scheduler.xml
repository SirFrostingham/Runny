<?xml version="1.0" encoding="utf-8" ?>

<!--
startDate       Can be NULL/empty   - example: null, 1/1/2015
endDate         Can be NULL/empty   - example: null, 1/10/2015
time            Must be miliarty time format: 00:00:00 - 23:59:59
day             1,2,3,4,5,6,7   =   Sun,Mon,Tues,Wed,Thurs,Fri,Sat
loop            1 or 0  =   On or Off
command         Executable, script or whatever command to run
commandParams   command parameters
comment         note on command -   example: This is a wonderful command that I run every day at 4pm
-->

<scheduler>
    <job>
        <startDate></startDate>
        <endDate></endDate>
        <time>17:45:10</time>
        <day>1,2,3,4,5,6,7</day>
        <loop>1</loop>
        <command>C:\windows\notepad.exe</command>
        <commandParams></commandParams>
        <comment>Run notepad</comment>
    </job>
    <job>
        <startDate></startDate>
        <endDate></endDate>
        <time>15:02:45</time>
        <day>1,2,3,4,5,6,7</day>
        <loop>1</loop>
        <command>C:\windows\notepad.exe</command>
        <commandParams></commandParams>
        <comment>Run notepad</comment>
    </job>
    <job>
        <startDate></startDate>
        <endDate></endDate>
        <time>20:10:00</time>
        <day>1,2,3,4,5,6,7</day>
        <loop>1</loop>
        <command>C:\windows\notepad.exe</command>
        <commandParams></commandParams>
        <comment>Run notepad again</comment>
    </job>
</scheduler>